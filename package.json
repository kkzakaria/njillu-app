{
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true pnpm build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky",
    "setup:email-templates": "tsx scripts/setup-email-templates.ts",
    "test:email-templates": "tsx scripts/test-templates.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=components",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:cross-browser": "playwright test e2e/cross-browser-compatibility.spec.ts",
    "test:accessibility": "playwright test e2e/accessibility.spec.ts",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "validate:browser-compatibility": "node scripts/validate-browser-compatibility.js",
    "validate:compatibility:ci": "node scripts/validate-browser-compatibility.js chromium firefox webkit",
    "check:security": "pnpm audit --audit-level high",
    "check:outdated": "pnpm outdated",
    "check:bundle-size": "npx @next/bundle-analyzer",
    "db:generate-types": "supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > types/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "health-check": "node scripts/health-check.js"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cobe": "^0.6.4",
    "date-fns": "^4.1.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.534.0",
    "motion": "^12.23.11",
    "next": "latest",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-aria-components": "^1.11.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-phone-number-input": "^3.4.12",
    "tailwind-merge": "^3.3.0",
    "use-mask-input": "^3.5.0",
    "@vercel/analytics": "^1.3.0",
    "@vercel/speed-insights": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.0.0",
    "@next/eslint-plugin-next": "^15.4.5",
    "@playwright/test": "^1.48.0",
    "@sentry/nextjs": "^8.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
